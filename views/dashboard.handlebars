
{{#if userBlogPosts.length}}
<section>
  <h2>Your Posts</h2>
    {{#each userBlogPosts as |post|}}
      {{> dashboard-postinfo post}}
    {{/each}}
</section>
{{/if}}

<button type="submit" class="btn"><a class="link" href="/dashboard/new">+ New Post</a></button>

<script src="/js/addBlogPost.js"></script>
